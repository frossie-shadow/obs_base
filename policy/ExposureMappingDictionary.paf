#<?cfg paf dictionary ?>

target: Mapping

definitions: {
    template: {
        type: string
        description: "Python substitution template for the path"
        minOccurs: 1
        maxOccurs: 1
    }
    python: {
        type: string
        description: "The Python type for this dataset type"
        minOccurs: 1
        maxOccurs: 1
    }
    persistable: {
        type: string
        description: "The Persistable registration for this dataset type"
        minOccurs: 1
        maxOccurs: 1
    }
    storage: {
        type: string
        description: "The Persistable registration for this dataset type"
        default: "FitsStorage"
        maxOccurs: 1
    }
    level: {
        type: string
        description: "The camera hierarchy level for this dataset type"
        maxOccurs: 1
    }
    tables: {
        type: string
        description: "Tables to use for queries of the registry"
        minOccurs: 1
    }
    columns: {
        type: string
        description: "Columns to use for queries of the registry"
    }
    recipe: {
        type: policy
        description: "Recipe name for writing"
    }
}
